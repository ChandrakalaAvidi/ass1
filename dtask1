#include<stdio.h>
#include<conio.h>
main()
{
	int a[5],i,small,n;
	printf("enter the arrya size:");
	scanf("%d",&n);
	printf("enter the array elements:");
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
	small=a[0];
	for(i=0;i<n;i++)
	{
	if(a[i]<small)
	small=a[i];
    }
	printf("\nsmall=%d",small);
	getch();
}
